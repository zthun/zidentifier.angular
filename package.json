{
    "name": "@zthun/zidentifier.angular",
    "version": "1.1.0",
    "description": "The zidentifier feature for Angular4+",
    "keywords": [
        "zid",
        "zthunworks"
    ],
    "author": "Anthony Bonta",
    "license": "MIT",
    "main": "src/index.js",
    "types": "src/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/zthun/zid.angular"
    },
    "scripts": {
        "clean:src": "rimraf src/*.d.ts src/**/*.d.ts src/*.js src/**/*.js src/*.map src/**/*.map",
        "clean:bin": "rimraf bin *.tgz",
        "clean": "npm run clean:bin && npm run clean:src",
        "test": "karma start cfg/karma.conf.js",
        "lint:ts": "tslint -c node_modules/zwebstyles/tslint.json src/**/*.ts src/*.ts test/*.ts",
        "lint:js": "eslint cfg/*.js --config=node_modules/zwebstyles/.eslintrc",
        "lint": "npm run lint:ts && npm run lint:js",
        "compile:webpack": "webpack --config cfg/webpack.config",
        "compile:ts": "tsc",
        "compile": "npm run compile:ts && npm run compile:webpack",
        "compress": "uglifyjs bin/dist/zidentifier.js -o bin/dist/zidentifier.min.js",
        "make": "npm run clean && npm run lint && npm run test && npm run compile && npm run compress",
        "push": "git push origin && git push origin --tags && npm publish --access=public"
    },
    "peerDependencies": {
        "@angular/core": "^4.4.3 < 6.0.0",
        "core-js": "^2.5.0"
    },
    "devDependencies": {
        "@angular/common": "=5.1.3",
        "@angular/compiler": "=5.1.3",
        "@angular/core": "=5.1.3",
        "@angular/platform-browser": "=5.1.3",
        "@angular/platform-browser-dynamic": "=5.1.3",
        "@types/jasmine": "=2.8.3",
        "@types/node": "=9.3.0",
        "awesome-typescript-loader": "=3.4.1",
        "core-js": "=2.5.3",
        "eslint": "=4.15.0",
        "jasmine-core": "=2.8.0",
        "karma": "=2.0.0",
        "karma-jasmine": "=1.1.1",
        "karma-jasmine-html-reporter": "=0.2.2",
        "karma-junit-reporter": "=1.2.0",
        "karma-phantomjs-launcher": "=1.0.4",
        "karma-typescript": "=3.0.9",
        "phantomjs-prebuilt": "=2.1.16",
        "rimraf": "=2.6.2",
        "rxjs": "=5.5.6",
        "tslint": "=5.8.0",
        "typescript": "=2.6.2",
        "webpack": "=3.10.0",
        "zone.js": "=0.8.19",
        "zwebstyles": "=3.0.0"
    },
    "dependencies": {
        "@zthun/zidentifier.core": "=1.0.0"
    }
}
